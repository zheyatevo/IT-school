import java.util.Scanner;
public class Main {
    public static void main(String[] argv) {
        Scanner in = new Scanner(System.in);
        int n = in.nextInt();
        int x = 10;
        int f =0;

        while (n >0){

            f=f+(n%x);
          n=(n-n%10)/10;
          
        }System.out.print(f);
        }
    }

