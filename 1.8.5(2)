import java.util.Scanner;
public class Main {
    public static void main(String[] args) {

        Scanner in = new Scanner(System.in);
        int n = in.nextInt();
        int []a = new int [n];
        int sum =0; int f =0; int y =0;
        for(int i=0;i<n;i++){
            a[i]= in.nextInt();}
        for(int i=0;i<n-1;i++){
            f=a[i]; y=a[i+1];
            if (y>f){ sum =sum+1;} }
System.out.print(sum);
        }
    }

