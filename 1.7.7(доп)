import java.util.Scanner;
public class Main {
    public static void main(String[] argv) {
        Scanner in = new Scanner(System.in);
        int a = in.nextInt();
        int c=0;
        for(int i=1;i<a;i++){

            if(a%i==0){c=c+1;}

        }
        if(c>2){System.out.print("composite");}
        else{System.out.print("prime");}
    }

}

