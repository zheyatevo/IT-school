import java.util.Scanner;
public class Main {
    public static void main(String[] argv) {
        Scanner in = new Scanner(System.in);
        int n = in.nextInt();
        int x = 0;
        int f =0;
        while (x < n){
           f=f+1;
           if(f%2==0){System.out.print(f+" "); x=x+1;}
        }
        }
    }
